<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AvaloniaApplication1.UserControl1"
			 xmlns:vm="using:AvaloniaApplication1.ViewModels"
			 x:DataType="vm:MainWindowViewModel">
	
	<StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
		<TextBlock Text="Вход" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="30" FontWeight="Black" Padding="30"/>
		<Button Command="{Binding Enter}" Width="150" Height="35" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="Lavender">Войти</Button>
		<Button Command="{Binding Registrainon}" Width="150" Height="35" VerticalAlignment="Center" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="Lavender">Регистрация</Button>
	</StackPanel>
	
</UserControl>
